        "Validators": {
            "FirstNameValidator": {
                "Regex": "^\\p{L}{2}(\\p{L}| ){0,254}$",
                "Description": "SignUp FirstName"
            },
            "LastNameNameValidator": {
                "Regex": "^\\p{L}{2}(\\p{L}| ){0,254}$",
                "Description": "SignUp LastName"
